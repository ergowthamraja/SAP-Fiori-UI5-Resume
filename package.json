{
    "name": "SAP_FIORI_RESUME",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/cds-odata-v2-adapter-proxy": "^1.9.7",
        "express": "^4"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1.3.1",
        "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
        "@sap/ux-specification": "^1.108.18",
        "@sapui5/ts-types": "^1.92.2",
        "eslint": "^7.32.0",
        "rimraf": "^3.0.2",
        "sqlite3": "^5.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-sap_fiori_resume": "cds watch --open sap_fiori_resume/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy SAP_FIORI_RESUME --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-sapresumeapp": "cds watch --open sapresumeapp/webapp/index.html?sap-ui-xx-viewCache=false",
        "watch-sap_resume_app": "cds watch --open sap_resume_app/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "cds": {
        "requires": {
            "db": "sqlite"
        }
    }
}